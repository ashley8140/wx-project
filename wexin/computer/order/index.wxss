/**index.wxss**/.page-title{    line-height:1.5; padding: 18rpx 30rpx; background-color:#f2f2f2; color:#999;}.summary-info{    padding: 24rpx 30rpx;}.serial-logo{    display:block; width:216rpx; height:144rpx;}.summary-info-right{ width:440rpx; margin-left:30rpx; }.serial-name{    font-size:36rpx; color:#333; display:block; padding-top:36rpx;}.serial-name-with-computer{ padding-top:26rpx; }.serial-computer-name{ font-size:28rpx; padding-top:4rpx; display:block;  color:#999; }.view-form{ padding:0 24rpx; box-size:border-box; }.input-view{ padding: 28rpx 0 28rpx; border-top:1px solid #eee; }.input-span{ font-size:32rpx; width:110rpx; height:50rpx; line-height:50rpx;  }.input-inp{ width: 590rpx; height:50rpx; line-height:50rpx; font-size:32rpx; color:#666; }.place-holder{ color:#aaa; font-size:24rpx;}.input-inp-city{    font-size:32rpx;    background: url("data:image/png;base64,") right center no-repeat;    background-size: 16rpx;}.submit-button{ height:90rpx; line-height:90rpx; margin-top:40rpx; background-color:#ee4653; }.select-city-mask{ position: fixed; top:0; left: 0; width:100%; height: 100%; background: rgba(0,0,0,.8); z-index: 90; }.city-pick-container{ position:absolute; left:0; bottom:0; width:100%; background:#fff; }.action-top{ background:#f5f5f5; padding:0 30rpx; }.pick-cancel{ font-size:30rpx; display:block; height:90rpx; line-height:90rpx; padding:0 20rpx; color:#666; }.pick-ok{ font-size:30rpx; display:block; height:90rpx; line-height:90rpx; padding:0 20rpx; color:#50bf3b;  }.city-picker{ width:100%; height:380rpx;  }.select-item{ line-height:68rpx; text-align:center; font-size:30rpx; }.success-toast{    position: fixed; top:0; left: 0; width:100%; height: 100%; background: rgba(0,0,0,.8); z-index: 100;}.modal-box{    width: 500rpx; border-radius:10rpx; padding-top: 152rpx; padding-bottom: 60rpx;    background: #fff url('data:image/png;base64,') center 50rpx no-repeat;}.result-tip{    font-size: 34rpx; color: #EE4653; text-align: center;}.result-tip2{    font-size: 24rpx; color: #999; margin: 10rpx auto 0;  text-align: center;}/*经销商*/.deal-container{    padding-bottom: 90rpx;}.deal-title{ height:72rpx; line-height:72rpx; background: #F9F9F9; padding-left:30rpx; color:#333; }.deal-list{padding:  0 24rpx;}.deal-item{    padding: 34rpx 0 34rpx 70rpx; display: flex; align-items: center; justify-content:space-between;    border-bottom: 1px solid #f5f5f5;    background: url('data:image/png;base64,') left center no-repeat; background-size:40rpx;}.deal-item.active{ background-image:url('data:image/png;base64,'); }.deal-left{ width: 454rpx; }.shop-name{ font-size:34rpx; color:rgb(51, 51, 51); }.shop-4s{    font-size: 22rpx; color: rgb(101, 123, 221); height: 32rpx; line-height: 28rpx; border: 1px solid rgb(100, 127, 195);    padding:0 10rpx; vertical-align: middle;}.shop-addr{ font-size: 28rpx; color: #999; line-height: 1.3; margin-top: 16rpx; }.deal-right{ font-size: 30rpx; }.show-dearlist .button-view{    position:fixed; left:0; bottom:0; width:100%; padding:12rpx 24rpx; box-sizing:border-box; background:#fff;}.show-dearlist .submit-button{    margin:0; padding:0;}